@model List<AcunMedyaAkademiPortfolyo.Models.Adress>

<div class="col-md-4 d-flex pl-md-5">
    <div class="row">
        @foreach (var item in Model) {
        <div class="dbox w-100 d-flex">
            <div class="icon d-flex align-items-center justify-content-center">
                <span class="fa fa-map-marker"></span>
            </div>
            <div class="text">
                <p><span>Adres:</span>@item.Adres</p>
            </div>
        </div>
        <div class="dbox w-100 d-flex">
            <div class="icon d-flex align-items-center justify-content-center">
                <span class="fa fa-phone"></span>
            </div>
            <div class="text">
                <p><span>Telefon No:</span> <a href="tel://1234567920">@item.Phone</a></p>
            </div>
        </div>
        <div class="dbox w-100 d-flex">
            <div class="icon d-flex align-items-center justify-content-center">
                <span class="fa fa-paper-plane"></span>
            </div>
            <div class="text">
                <p><span>Email:</span> <a href="mailto:info@yoursite.com">@item.Email</a></p>
            </div>
        </div>
        <div class="dbox w-100 d-flex">
            <div class="icon d-flex align-items-center justify-content-center">
                <span class="fa fa-globe"></span>
            </div>
            <div class="text">
                <p><span>Website:</span> <a href="@item.Website" target="_blank">@item.Website</a></p>
            </div>
        </div>
        }
    </div>
</div>
