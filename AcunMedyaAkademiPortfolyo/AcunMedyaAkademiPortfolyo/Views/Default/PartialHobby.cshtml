@model List<AcunMedyaAkademiPortfolyo.Models.Hobby>

    @foreach (var item in Model)
    {
        <div class="interest-wrap d-flex align-items-center">
            <div class="icon d-flex align-items-center justify-content-center" style="background-color: white; border-radius: 50%;">
                <span><img src="@item.IconUrl" style="width:25px;height:25px;" /></span>
            </div>
            <div class="text">@item.Title</div>
        </div>
    }
